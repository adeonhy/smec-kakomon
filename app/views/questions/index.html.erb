

<h2><%= @year %>年度 <%= @subject %></h2>
<ul class="thumbnails">
    <% @questions.each do |q| %>
        <li class="span3">
        <div class="thumbnail">
            <%= link_to (image_tag "/assets/svg/#{q.svg}", height:"100"), "/q/#{@kamokucode}/#{q.year}/#{q.number}" %>
            <!-- <%= (image_tag "/assets/svg/#{q.svg}", height:"100") %> -->
            <div class="caption">
                <h5>第<%= q.number %>問</h5>
                <%= @themes[q.number].subtheme %>
            </div>
        </div>
        </li>
    <% end %>
</ul>
<%= link_to 'Topへ', '/' %>
